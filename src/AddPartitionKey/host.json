{
  "version": "2.0",
  "logger": {
    "categoryFilter": {
      "defaultLevel": "Information",
      "categoryLevels": {
        "Host.Executor": "Error",
        "Host.Results": "Error",
        "Function": "Debug"
      }
    }
  },
  "aggregator": {
    "batchSize": 1000,
    "flushTimeout": "00:00:30"
  },
  "applicationInsights": {
    "sampling": {
      "isEnabled": true,
      "maxTelemetryItemsPerSecond": 5
    }
  },
  "queues": {
    "maxDequeueCount": 10,
    "maxPollingInterval": 1000,
    "visibilityTimeout": "00:00:30",
    "batchSize": 16
  }
}